# Docker environment variables
COMPOSE_PROJECT_NAME=vehicle_management

# MySQL
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=vehicle_management
MYSQL_USER=vehicle_user
MYSQL_PASSWORD=vehicle_pass

# PHP/Symfony
APP_ENV=dev
APP_SECRET=changeme_secret_key_for_production
DATABASE_URL=mysql://vehicle_user:vehicle_pass@mysql:3306/vehicle_management

# JWT
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=changeme

# SAML/SSO
SAML_IDP_ENTITY_ID=
SAML_IDP_SSO_URL=
SAML_IDP_X509_CERT=

# Frontend
REACT_APP_API_URL=/api
