{
  "key": "consumables_due",
  "name": "Consumables Due",
  "description": "Consumables that are due for replacement",
  "filename": "Consumables_Due",
  "periods": [ {"label":"Next 1000 miles","months":0,"days":30},{"label":"All due","months":null} ],
  "defaultPeriodIndex": 0,
  "outputs": [{"mode":"screen"},{"mode":"file","fileType":"xlsx"}],
  "dataSources": {
    "consumables": {
      "entity": "Consumable",
      "filter": { "due": true }
    }
  },
  "sheets": [
    {
      "name": "Consumables Due",
      "source": "consumables",
      "columns": [
        {"key":"registration","label":"Vehicle","width":15},
        {"key":"item","label":"Item","width":30},
        {"key":"currentMileage","label":"Current Mileage","width":18,"format":"number"},
        {"key":"nextReplacement","label":"Due at Mileage","width":18,"format":"number"},
        {"key":"date","label":"Last Changed","width":15,"format":"date"}
      ]
    }
  ]
}
